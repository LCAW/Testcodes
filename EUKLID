#include<stdio.h>
int main (){
    
        int x,y;
        printf("Geben Sie ein Wert für x ein\n");
        scanf("%d", &x);
        printf("Geben Sie ein Wert für y ein\n");
        scanf("%d", &y);
        while(x!=y){
            if(x>y){
                x=x-y;
            }else{
                y=y-x;
            }
        }
        printf("\nDer gesuchte Wert ist %d \n",x);
return 0;
}
